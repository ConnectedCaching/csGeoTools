    <groundspeak:cache id="1956513" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Santa Fe</groundspeak:name>
      <groundspeak:placed_by>Merinoo</groundspeak:placed_by>
      <groundspeak:owner id="2546976">Merinoo</groundspeak:owner>
      <groundspeak:type>Unknown Cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
